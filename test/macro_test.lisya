
(block
    (fmt nil "макросы ...  ")
    (macro m (a b) (list \var a b))
    (m v "var")
    (assertion (= "var" v) "macro var")

    (macro plus (:rest args) (concatenate (list +) args))
    (assertion (= 6 (plus 1 2 3)) "macro eval")

    (macro-symbol tst (list + 1 2))
    (assertion (= 3 tst) "macro-symbol")

    (log "успешно"))
